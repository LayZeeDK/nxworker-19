{
  "name": "github-actions-post-status-check",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": ".github/actions/post-status-check/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/.github/actions/post-status-check"],
      "defaultConfiguration": "production",
      "options": {
        "command": "node .github/actions/post-status-check/build.js",
        "cwd": "{workspaceRoot}"
      },
      "configurations": {
        "development": {
          "command": "NODE_ENV=development node .github/actions/post-status-check/build.js"
        },
        "production": {
          "command": "NODE_ENV=production node .github/actions/post-status-check/build.js"
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": ["build"],
      "options": {
        "buildTarget": "github-actions-post-status-check:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "github-actions-post-status-check:build:development"
        },
        "production": {
          "buildTarget": "github-actions-post-status-check:build:production"
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": ".github/actions/post-status-check/jest.config.js"
      }
    }
  }
}
